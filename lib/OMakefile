################################################
# Build an OCaml library
#

FILES[] =
  utils
  matrix
  matrix_op
  linearEq
  type
  eval
  differentiation
  integral
  taylor
  nonLinearEq
  l4nc

LIB = l4nc
BYTE_ENABLED = true
NATIVE_ENABLED = true

.PHONY: build clean toplevel install uninstall

.DEFAULT: $(OCamlLibrary $(LIB), $(FILES))

clean:
	rm -f *.cm* *.a *.o
