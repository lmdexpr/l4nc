################################################
# Build an OCaml library
#

FILES[] =
  utils
  matrix
  matrix_op
  linearEq
  type
  differentiation
  taylor
  nonLinearEq
  l4nc

LIB = l4nc

.PHONY: clean install uninstall

.DEFAULT: $(OCamlLibrary $(LIB), $(FILES))

clean:
	rm -f *.cm* *.a *.o
